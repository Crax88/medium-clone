{
	"name": "frontend",
	"version": "1.0.0",
	"description": "Medium clone frontend with React, Typescript and Webpack",
	"main": "src/index.ts",
	"scripts": {
		"start": "webpack serve --env port=3000 mode=development",
		"build:prod": "webpack --env mode=production",
		"build:dev": "webpack --mode=development",
		"build:analyze": "webpack --env mode=production analyzerMode=analyze",
		"lint:fixAll": "npm run lint:fix && npm run format:fix",
		"lint": "eslint \"**/*.{ts,tsx}\"",
		"lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
		"format": "prettier --check \"**/*.*\"",
		"format:fix": "prettier --write \"**/*.*\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Crax88/medium-clone.git"
	},
	"homepage": "https://github.com/Crax88/medium-clone#readme",
	"keywords": [
		"React",
		"Webpack",
		"Typescript"
	],
	"author": "N. Trafilkin",
	"license": "MIT",
	"dependencies": {
		"@linaria/babel-preset": "4.3.0",
		"@linaria/core": "4.2.2",
		"@linaria/react": "4.3.0",
		"@types/react-router-dom": "5.3.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "6.4.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.19.3",
		"@babel/core": "^7.20.2",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@babel/runtime": "^7.20.1",
		"@linaria/webpack-loader": "4.1.8",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.8",
		"@types/webpack": "^5.28.0",
		"@types/webpack-bundle-analyzer": "^4.6.0",
		"@types/webpack-dev-server": "^4.7.2",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"babel-loader": "^9.1.0",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.2",
		"css-minimizer-webpack-plugin": "^4.2.2",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-webpack": "^0.13.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"fork-ts-checker-webpack-plugin": "7.2.13",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.6.1",
		"prettier": "^2.7.1",
		"react-refresh": "^0.14.0",
		"regenerator-runtime": "^0.13.10",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4",
		"webpack": "^5.75.0",
