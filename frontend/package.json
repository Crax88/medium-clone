{
	"name": "frontend",
	"version": "1.0.0",
	"description": "Medium clone frontend with React, Typescript and Webpack",
	"main": "src/index.ts",
	"scripts": {
		"start": "webpack serve --env port=3000 mode=development",
		"build:prod": "webpack --env mode=production",
		"build:dev": "webpack --mode=development",
		"build:analyze": "webpack --env mode=production analyzerMode=analyze",
		"lint:fixAll": "npm run lint:fix && npm run format:fix",
		"lint": "eslint ./src/**",
		"lint:fix": "eslint ./src/** --fix",
		"format": "prettier --check ./src/**",
		"format:fix": "prettier --write ./src/**",
		"prepare": "cd .. && husky install frontend/.husky",
		"lint-staged": ""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Crax88/medium-clone.git"
	},
	"homepage": "https://github.com/Crax88/medium-clone#readme",
	"keywords": [
		"React",
		"Webpack",
		"Typescript"
	],
	"author": "N. Trafilkin",
	"license": "MIT",
	"dependencies": {
		"@hookform/resolvers": "^3.1.0",
		"@reduxjs/toolkit": "^1.9.5",
		"marked": "^5.0.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.44.3",
		"react-redux": "^8.0.7",
		"react-router-dom": "^6.4.3",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.19.3",
		"@babel/core": "^7.20.2",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@babel/runtime": "^7.20.1",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
		"@svgr/webpack": "^8.0.1",
		"@total-typescript/ts-reset": "^0.4.2",
		"@types/marked": "^5.0.0",
		"@types/node": "^18.11.9",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.8",
		"@types/webpack": "^5.28.0",
		"@types/webpack-bundle-analyzer": "^4.6.0",
		"@types/webpack-dev-server": "^4.7.2",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"babel-loader": "^9.1.0",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.2",
		"css-minimizer-webpack-plugin": "^4.2.2",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-webpack": "^0.13.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-webpack-plugin": "^4.0.1",
