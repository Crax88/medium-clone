{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Medium clone frontend with React, Typescript and Webpack",
  "main": "src/index.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.ts --env env=development",
    "build": "webpack --config webpack.config.ts --env env=production",
    "build:dev": "webpack --config webpack.config.ts --env env=development",
    "analyze": "npm run build && webpack-bundle-analyzer stats.json dist",
		"lint": "eslint ./src/**",
		"lint:fix": "eslint ./src/**/*",
		"format": "prettier --write ./src/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crax88/medium-clone.git"
  },
  "homepage": "https://github.com/Crax88/medium-clone#readme",
  "keywords": [
    "React",
    "Webpack",
    "Typescript"
  ],
  "author": "N. Trafilkin",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.1",
    "@types/css-minimizer-webpack-plugin": "^3.2.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-dev-server": "^4.7.2",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
		"prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
